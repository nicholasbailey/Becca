
def fizzBuzz(numberOfIterations) {
    n = 0;

    while (n < numberOfIterations) {
        if ((n % 3 == 0) && (n % 5 == 0)) {
            print("FizzBuzz");
        } else if (n % 3 == 0) {
            print("Fizz");
        } else if (n % 5 == 0) {
            print("Buzz");
        } else {
            print(n);
        }
        n = n + 1;
    }
}

fizzBuzz(20);
print("Fizzbuzz Test Passed")